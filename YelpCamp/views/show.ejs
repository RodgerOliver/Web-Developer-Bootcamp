<% include partials/header %>

<div class="container">
	<h1><%= camp.name %></h1>
	<img src="<%= camp.image %>">
	<p><%= camp.description %></p>
	<% camp.comments.forEach(function(comment) { %>
	<p><strong><%= comment.author %></strong> - <%= comment.text %></p>
	<% }); %>
</div>

<% include partials/footer %>